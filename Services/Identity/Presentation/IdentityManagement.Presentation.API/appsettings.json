{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "Jwt": {
    "Issuer": "issuer",
    "Audience": "audience",
    "AccessTokenSecurityKey": "access-token-security-key",
    "AccessTokenLifeTimeInMinute": 5,
    "RefreshTokenSecurityKey": "refresh-token-security-key",
    "RefreshTokenLifeTimeInMinute": 43200
  },
  "OpenApi": {
    "Document": {
      "Title": "Identity",
      "Version": "v1"
    }
  },
  "EventBus": {
    "SubscriptionClientName": "Identity",
    "RetryCount": 5,
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "Port": 5672
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5003"
      },
      "gRPC": {
        "Url": "http://localhost:50051",
        "Protocols": "Http2"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "ServiceName": "Identity"
    }
  },
  "CORS": "Identity",
  "Database": {
    "Host": "localhost",
    "Port": "5432",
    "Database": "Identity",
    "Username": "postgres",
    "Password": "password"
  },
  "Email": {
    "DisplayName": "",
    "Host": "smtp.gmail.com",
    "Port": 465,
    "Username": "",
    "Password": ""
  }
}
